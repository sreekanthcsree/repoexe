<web-app>
  <display-name>Archetype Created Web Application</display-name>
 
  <welcome-file-list>
        <welcome-file>Welcome.html</welcome-file>  
  </welcome-file-list>
  
  <servlet>
    <servlet-name>WS</servlet-name>
    <servlet-class>com.dex.WelcomeServlet</servlet-class>
</servlet>
  <servlet-mapping>
     <servlet-name>WS</servlet-name>
     <url-pattern>/wser</url-pattern>
  </servlet-mapping>
</web-app>
